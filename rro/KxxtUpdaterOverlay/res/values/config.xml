<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- URL to be checked for OTA updates -->
    <!-- device: ro.lineage.device e.g. lmi -->
    <!-- type: ro.lineage.releasetype e.g. UNOFFICIAL -->
    <!-- incr: ro.build.version.incremental e.g. 1751107720 -->
    <string name="updater_server_url" translatable="false">https://github.com/android-kxxt/ota/raw/refs/heads/main/{device}-{type}.json</string>

    <!-- URL to changelog -->
    <!-- Use SSG to generate a changelog page after device build -->
    <string name="menu_changelog_url" translatable="false">https://android.kxxt.dev/changes/<xliff:g id="device_name">%1$s</xliff:g>/</string>
</resources>